package org.campitos.capitulo1;

/**
 *
 * @author campitos
 */
public class Imagen {

private int alto;
private int ancho;

public String getAlto() {
        return alto;
    }

    public void setAlto(int alto) {
        if(alto<0)alto=-alto;
        this.alto = alto;
    }

    public int Ancho() {
        return ancho;
    }

    public void setAncho(int ancho) {
        if(ancho<0)ancho=-ancho;
        this.ancho = ancho;
    }
    
}
